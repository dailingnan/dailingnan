<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.eleme.menumanage.dao.StyleFoodDao">
    <select id="selectStyle" resultType="foodd">
    	select * from stylefood
    </select>
    <update id="updateStyle" parameterType="foodd">
    	update stylefood set sname=#{sname} where sid=#{sid}
    </update>
    </mapper>